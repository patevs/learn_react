{
  "name": "learn_react",
  "version": "0.9.0",
  "description": "Links, resources, and code related to my learning of the React frontend JavaScript framework",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/learn_react.git"
  },
  "scripts": {
    "start": "cd react-router && react-scripts start",
    "build": "cd react-router && react-scripts build",
    "test": "cd react-router && cross-env CI=true react-scripts test --all",
    "eject": "cd react-router && react-scripts eject",
    "release": "np"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "commitlint-config-gitmoji": "^2.2.3",
    "cross-env": "^7.0.3",
    "husky": "^4.3.8",
    "np": "^7.4.0",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "commitlint": {
    "extends": [
      "./node_modules/commitlint-config-gitmoji"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
